{
  "name": "guillerming/guacamole",
  "description": "Modern php strict typed non-opinionated framework",
  "type": "project",
  "require": {
    "vlucas/phpdotenv": "^5.6"
  },
  "scripts": {},
  "config": {
    "process-timeout": 0
  },
  "autoload": {
    "psr-4": {
      "Guacamole\\": [
        "src/Guacamole/",
        "dist/Guacamole/"
      ],
      "Site\\": [
        "src/Site/",
        "dist/Site/"
      ]
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "phpstan/phpstan": "^2.1"
  }
}
